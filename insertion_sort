#include <iostream>

using namespace std;
void insertionSort(int ar_size, int * ar) {
//Write your code here.
int i, key, j;
   for (i = 1; i < ar_size; i++)
   {
       key = ar[i];
       j = i-1;
       while (j >= 0 && ar[j] > key)
       {
           ar[j+1] = ar[j];
           j = j-1;
       }
       ar[j+1] = key;
       for(int len=0;len<ar_size;len++)
       {
       	cout<<ar[len]<<"  ";
	   }
	   cout<<endl;
   }
}
int main(void) {
int _ar_size;
cin>>_ar_size;
int _ar[_ar_size], _ar_i;
for(_ar_i = 0; _ar_i < _ar_size; _ar_i++) {
cin>>_ar[_ar_i];
}
insertionSort(_ar_size, _ar);
return 0;
}
